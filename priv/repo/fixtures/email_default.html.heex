                                          <%= if have_discount? do %>
                                            <th style="Margin: 0; background-color: #e7e7e2; border-bottom: 1px solid #cbcbc0; color: #3d3f3e; font-family: Helvetica, Arial, sans-serif; font-size: 14px; font-weight: bold; line-height: 1.5; margin: 0; padding: .625rem; text-align: left">
                                              Price
                                            </th>
                                            <th style="Margin: 0; background-color: #e7e7e2; border-bottom: 1px solid #cbcbc0; color: #3d3f3e; font-family: Helvetica, Arial, sans-serif; font-size: 14px; font-weight: bold; line-height: 1.5; margin: 0; padding: .625rem; text-align: left">
                                              Discount
                                            </th>
                                          <% end %>
                                          <th class="total" style="Margin: 0; background-color: #e7e7e2; border-bottom: 1px solid #cbcbc0; color: #3d3f3e; font-family: Helvetica, Arial, sans-serif; font-size: 14px; font-weight: bold; line-height: 1.5; margin: 0; padding: .625rem; text-align: right; white-space: nowrap">
                                            Total
                                          </th>
                                        </tr>
                                      </thead>
                                      <tbody>
                                        <%= for item <- items do %>
                                          <tr style="padding: 0; text-align: left; vertical-align: top">
                                            <td style="-moz-hyphens: auto; -webkit-hyphens: auto; Margin: 0; border-collapse: collapse !important; color: #3d3f3e; font-family: Helvetica, Arial, sans-serif; font-size: 14px; font-weight: normal; hyphens: auto; line-height: 1.5; margin: 0; padding: .625rem; text-align: left; vertical-align: top; word-wrap: break-word">
                                              <%= item.description %>
                                            </td>
                                            <%= if have_discount? do %>
                                              <td style="-moz-hyphens: auto; -webkit-hyphens: auto; Margin: 0; border-collapse: collapse !important; color: #3d3f3e; font-family: Helvetica, Arial, sans-serif; font-size: 14px; font-weight: normal; hyphens: auto; line-height: 1.5; margin: 0; padding: .625rem; text-align: left; vertical-align: top; word-wrap: break-word">
                                                  <%= SiwappWeb.PageView.money_format(item.unitary_cost, currency) %>
                                              </td>
                                              <td style="-moz-hyphens: auto; -webkit-hyphens: auto; Margin: 0; border-collapse: collapse !important; color: #3d3f3e; font-family: Helvetica, Arial, sans-serif; font-size: 14px; font-weight: normal; hyphens: auto; line-height: 1.5; margin: 0; padding: .625rem; text-align: left; vertical-align: top; word-wrap: break-word">
                                                <%= if item.discount != 0 do %>
                                                  <%= item.discount %>
                                                <% end %>
                                              </td>
                                            <% end %>
                                            <td class="total" style="-moz-hyphens: auto; -webkit-hyphens: auto; Margin: 0; border-collapse: collapse !important; color: #3d3f3e; font-family: Helvetica, Arial, sans-serif; font-size: 14px; font-weight: normal; hyphens: auto; line-height: 1.5; margin: 0; padding: .625rem; text-align: right; vertical-align: top; white-space: nowrap; word-wrap: break-word">
                                                <%= SiwappWeb.PageView.money_format(item.net_amount, currency) %>
                                            </td>
                                          </tr>
                                        <% end %>
                                      </tbody>
                                    </table>
                                  </th>
                                  <th class="expander" style="Margin: 0; color: #3d3f3e; font-family: Helvetica, Arial, sans-serif; font-size: 14px; font-weight: normal; line-height: 1.5; margin: 0; padding: 0 !important; text-align: left; visibility: hidden; width: 0">
                                  </th>
                                </tr>
                              </table>
                            </th>
                          </tr>
                        </tbody>
                      </table>
                      <br>
                      <table class="row" style="border-collapse: collapse; border-spacing: 0; display: table; padding: 0; position: relative; text-align: left; vertical-align: top; width: 100%">
                        <tbody>
                          <tr style="padding: 0; text-align: left; vertical-align: top">
                            <th class="small-12 large-6 columns first" style="Margin: 0 auto; color: #3d3f3e; font-family: Helvetica, Arial, sans-serif; font-size: 14px; font-weight: normal; line-height: 1.5; margin: 0 auto; padding: 0; padding-bottom: 20px; padding-left: 16px; padding-right: 8px; text-align: left; width: 9.07667cm">
