<h1> Settings </h1>
<%= form_for :key_values, Routes.settings_path(@conn, :update), fn f-> %>
  <%= for setting <- @settings do %>
    <p>
      <br>
      <%= label do setting.key end %>
      <br>
      <%= text_input f, setting.key , value: setting.value %>
      <br>
    </p>
  <% end %>
<%= submit "Save" %>
<% end %>
